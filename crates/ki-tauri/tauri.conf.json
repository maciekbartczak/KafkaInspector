{
  "productName": "kafka_inspector_v2",
  "version": "0.1.0",
  "identifier": "com.kafka_inspector_v2.app",
  "build": {
    "beforeDevCommand": "pnpm start",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist/kafka_inspector_v2/browser"
  },
  "app": {
    "windows": [
      {
        "title": "kafka_inspector_v2",
        "width": 1920,
        "height": 1080
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
